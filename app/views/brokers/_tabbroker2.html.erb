<div class="btn-toolbar">
  <div id="nav-broker-cust" class="btn-group nav-tabs" data-toggle="buttons-radio">
		<button class="btn"><i class="icon-search"></i></button>
		<% Time.now.beginning_of_year.strftime("%Y%m").upto(Time.now.end_of_year.strftime("%Y%m")).each do |i| %>
			<%= content_tag "button", :class => "btn", :value => i do %>
				<%= i.at(4..5) %>
			<% end %>
		<% end %>
  </div>
</div>
<div>
	<div class="well">
		<div id="chart_1">
			<%= render 'brokers/brokerchart1' %>
		</div>
	</div>
	<div class="row-fluid">
		<div class="span6">
			<div class="well">
				<div id="chart_2">
					<%= render 'brokers/brokerchart2' %>
				</div>
			</div>
		</div>
		<div class="span6">
			<div class="well">
				<div id="chart_3">
					<%= render 'brokers/brokerchart3' %>
				</div>
			</div>
		</div>
	</div>
</div>